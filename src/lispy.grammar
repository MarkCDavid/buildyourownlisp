integer     : /-?\d+\b/;
decimal     : /-?(\d+\.\d*)+/ ;
number      : <decimal> | <integer> ;
symbol      : /[a-zA-Z0-9_+\-*\/\\=<>!&\|]+/ ;
string      : /"(\\.|[^"])*"/;
comment     : /;[^\r\n]*/;
qexpression : '{' <expression>* '}' ;
sexpression : '(' <expression>* ')' ;
expression  : <number> | <symbol> | <string> | <comment> | <sexpression> | <qexpression> ;
lispy       : /^/ <expression>* /$/ ;