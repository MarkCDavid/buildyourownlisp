integer     : /-?\d+\b/;
decimal     : /-?(\d+\.\d*)+/ ;
number      : <decimal> | <integer> ;
symbol      : '+' | '-' | '*' | '/' | '%' 
            | "list" | "head" | "tail" | "eval" 
            | "join" | "cons" | "len"  | "init";
qexpression : '{' <expression>* '}' ;
sexpression : '(' <expression>* ')' ;
expression  : <number> | <symbol> | <sexpression> | <qexpression> ;
lispy       : /^/ <expression>* /$/ ;